  "devDependencies": {
    "@koa/router": "^12.0.0",
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa__router": "^12.0.0",
    "@types/lodash": "^4.17.14",
    "@types/md5": "^2.3.5",
    "koa-swagger-decorator": "^2.2.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
