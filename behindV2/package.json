  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "koa",
    "sequlize",
    "template"
  ],
  "author": "iceStone",
  "license": "MIT",
  "description": "nodejs的后端模板,使用了koa,typescript,koa-swagger-decorator,zod等",
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.7",
    "@types/koa-static": "^4.0.4",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "husky": "^9.1.7",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.13.1",
    "koa-body": "^6.0.1",
    "koa-bodyparser": "^4.4.1",
    "koa-onerror": "^4.2.0",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "lint-staged": "^15.4.1",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "md5": "^2.3.0",
    "module-alias": "^2.2.3",
    "mysql2": "^3.12.0",
