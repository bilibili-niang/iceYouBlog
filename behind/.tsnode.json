{
  "require": ["tsconfig-paths/register"],
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES6",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@services/*": ["src/services/*"],
      "@controllers/*": ["src/controllers/*"],
      "@utils/*": ["src/utils/*"],
      "@config/*": ["src/config/*"],
      "@constant/*": ["src/constant/*"]
    }
  },
  "files": false,
  "typeRoots": ["./node_modules/@types"]
}